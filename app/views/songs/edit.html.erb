<h1>edit a song!</h1>
<%= form_for(@song) do |f|%><br>
<label>title</label>
<%= f.text_field :title %><br>
<label>released</label>
<%= f.check_box :released %><br>
<label>release_year</label>
<%= f.text_field :release_year %><br>
<label>artist_name</label>
<%= f.text_field :artist_name %><br>
<label>genre</label>
<%= f.text_field :genre %><br>
<%= f.submit %>

<%end%>

<br><br>
<%if @song.errors.full_messages.length != 0%>
  <%@song.errors.full_messages.each do |el|%>
    <%=el%> <br>
  <%end%>
<%end%>
